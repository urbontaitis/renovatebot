{    
    "packageRules": [
        {
            "groupName": "spring-boot",
            "matchDatasources": "maven",
            "matchPackageNames": [
                "org.springframework.boot:spring-boot-starter-parent"
            ],
            "allowedVersions": "<2.4.0"
        },
        {
            "groupName": "non-major",
            "groupSlug": "non-major-dependencies",
            "matchPackagePatterns": [
                "*"
            ],
            "matchUpdateTypes": [
                "patch",
                "minor"
            ]
        },
        {
            "groupName": "MAJOR",
            "groupSlug": "major-dependencies",
            "matchPackagePatterns": [
                "*"
            ],
            "matchUpdateTypes": [
                "major"
            ]
        },
        {
            "registryUrls": [
                
            ],
            "matchManagers": [
                "maven",
                "npm"
            ]
        }
    ]
}